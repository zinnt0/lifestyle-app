{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": false
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "channel": "preview"
    },
    "production": {
      "autoIncrement": false,
      "ios": {
        "resourceClass": "m-medium"
      },
      "channel": "production"
    },
    "testflight": {
      "distribution": "store",
      "autoIncrement": true,
      "ios": {
        "resourceClass": "m-medium"
      },
      "channel": "testflight"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6758155309"
      }
    },
    "testflight": {
      "ios": {
        "ascAppId": "6758155309"
      }
    }
  }
}
